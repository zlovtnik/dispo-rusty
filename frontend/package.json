{
  "name": "actix-web-rest-api-frontend",
  "version": "1.0.0",
  "description": "TypeScript/Bun/React frontend for Actix Web REST API with JWT authentication and multi-tenant support - Natural Pharmacy Color System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun scripts/validate-env.js && vite build",
    "preview": "vite preview",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:ui": "bun test --preload=./happydom.ts",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "bun run type-check && bun run lint && bun run format:check"
  },
  "keywords": [
    "typescript",
    "bun",
    "react",
    "frontend",
    "jwt",
    "multi-tenant"
  ],
  "author": "Actix Web REST API Team",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "latest",
    "@types/qs": "^6.14.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "bun-plugin-tailwind": "^0.0.15",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "globals": "^16.4.0",
    "@happy-dom/global-registrator": "^20.0.1",
    "happy-dom": "^20.0.1",
    "msw": "^2.11.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.46.1",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^5.27.4",
    "fp-ts": "^2.16.11",
    "neverthrow": "^8.2.0",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.0.0",
    "react-router-dom": "^6.0.0",
    "ts-pattern": "^5.8.0",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-recipes": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zod": "^4.1.12"
  },
  "private": true
}