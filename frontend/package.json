{
  "name": "actix-web-rest-api-frontend",
  "version": "1.0.0",
  "description": "TypeScript/Bun/React frontend for Actix Web REST API with JWT authentication and multi-tenant support - Natural Pharmacy Color System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/validate-env.js && vite build",
    "build:bun": "bun scripts/validate-env.js && vite build",
    "preview": "vite preview",
    "test": "bun test --coverage",
    "test:watch": "bun test --watch",
    "test:ui": "bun test --dom"
  },
  "keywords": [
    "typescript",
    "bun",
    "react",
    "frontend",
    "jwt",
    "multi-tenant"
  ],
  "author": "Actix Web REST API Team",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/bun": "latest",
    "@types/qs": "^6.14.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "bun-plugin-tailwind": "^0.0.15",
    "eslint": "^9.37.0",
    "eslint-plugin-functional": "^9.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^5.27.4",
    "fp-ts": "^2.16.11",
    "neverthrow": "^8.2.0",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.0.0",
    "react-router-dom": "^6.0.0",
    "ts-pattern": "^5.8.0",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-recipes": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zod": "^4.1.12"
  },
  "private": true
}
